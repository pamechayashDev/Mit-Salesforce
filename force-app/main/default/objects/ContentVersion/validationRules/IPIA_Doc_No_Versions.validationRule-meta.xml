<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>IPIA_Doc_No_Versions</fullName>
    <active>true</active>
    <errorConditionFormula>RecordType.DeveloperName  = &apos;IPIA&apos; &amp;&amp; VALUE(VersionNumber) &gt; 1</errorConditionFormula>
    <errorMessage>An IPIA Document may not have File Versions. Please delete the document and add a new one.</errorMessage>
</ValidationRule>
